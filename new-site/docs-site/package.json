{
  "scripts": {
    "dev": "vuepress dev content --no-cache",
    "build": "vuepress build content --no-cache",
    "deploy": "yarn build && aws s3 cp --recursive ./content/.vuepress/dist s3://new-site.typesense.org/docs/ --profile typesense && yarn clearCDNCache && yarn scrapeAndIndexDocs",
    "clearCDNCache": "aws cloudfront create-invalidation --distribution-id EJ8VOUOZDDMVB --paths \"/docs*\" --profile typesense",
    "scrapeAndIndexDocs": "aws ecs run-task --task-definition=scrape-index-docs --launch-type=FARGATE --cluster=typesense-docsearch-scraper --overrides=\"{\\\"containerOverrides\\\":[ { \\\"name\\\": \\\"typesense-docsearch-scraper\\\", \\\"environment\\\": [ { \\\"name\\\": \\\"CONFIG\\\", \\\"value\\\": $(node docsearch.config.js | jq -aR .)}] }]}\" --network-configuration='{\"awsvpcConfiguration\": {\"subnets\": [\"subnet-00a6c52b\"], \"assignPublicIp\": \"ENABLED\"}}' --profile=typesense",
    "lint": "eslint --ext .js,.vue content/.vuepress",
    "lint:fix": "yarn lint --fix"
  },
  "devDependencies": {
    "@vuepress/plugin-back-to-top": "^1.8.2",
    "@vuepress/plugin-google-analytics": "^1.8.2",
    "@vuepress/plugin-medium-zoom": "^1.8.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "watchpack": "1.6.1"
  },
  "dependencies": {
    "@dovyp/vuepress-plugin-clipboard-copy": "^1.0.0-alpha.7",
    "vue-prism-component": "^1.2.0",
    "vuepress": "1.8.2",
    "vuex": "^3.6.2"
  }
}
