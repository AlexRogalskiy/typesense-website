{
  "name": "typesense.org",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "yarn generate && rm -rf ./dist/docs && aws s3 cp --recursive ./dist s3://new-site.typesense.org/ --profile typesense && yarn clearCDNCache",
    "clearCDNCache": "aws cloudfront create-invalidation --distribution-id EJ8VOUOZDDMVB --paths \"/*\" --profile typesense",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "clipboard": "^2.0.6",
    "lodash.startcase": "^4.4.0",
    "nuxt": "^2.14.12",
    "prism-themes": "^1.5.0",
    "prismjs": "^1.23.0",
    "typesense": "^0.11.1",
    "typesense-instantsearch-adapter": "^1.0.0",
    "vue-instantsearch": "^3.4.3"
  },
  "devDependencies": {
    "@nuxt/types": "~2.15.0",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "fibers": "^5.0.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "10"
  }
}
